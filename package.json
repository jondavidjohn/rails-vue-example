{
  "version": "0.1.0",
  "name": "rails_vue_example",
  "private": true,
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "rootDir": "./",
    "roots": [
      "<rootDir>/test/javascript",
      "<rootDir>/app/javascript"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/app/javascript/$1"
    },
    "transform": {
      "^.+\\.(js)$": "babel-jest",
      "^.*\\.(vue)$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  },
  "dependencies": {
    "@rails/actioncable": "^6.0.0-alpha",
    "@rails/activestorage": "^6.0.0-alpha",
    "@rails/ujs": "^6.0.0-alpha",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-loader": "^8.0.5",
    "turbolinks": "^5.2.0",
    "vue": "^2.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@rails/webpacker": "^4.0.2",
    "babel-jest": "^24.0.0",
    "jest": "^24.0.0",
    "jest-serializer-vue": "^2.0.2",
    "typescript": "^3.3.3",
    "vue-jest": "4.0.0-beta.1",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.6.3",
    "webpack-dev-server": "^3.2.1"
  }
}
